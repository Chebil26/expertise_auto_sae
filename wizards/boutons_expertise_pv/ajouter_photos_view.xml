<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_ajouter_photos_wizard" model="ir.ui.view">
        <field name="name">Wizard</field>
        <field name="model">ajouter.photos.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>

                    <group>
                        <field name="images" widget="many2many_binary"/>
                        <field name="note"/>
                    </group>

                </group>
                <footer>
                    <button name="confirmer" string="Confirmer" type="object" class="oe_highlight"/>
                    <button string="Annuler" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>